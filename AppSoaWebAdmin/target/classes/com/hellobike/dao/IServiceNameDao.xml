<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hellobike.dao.IServiceNameDao">
    <select id="findIdByServiceName" resultType="Integer" parameterType="serviceName">
        select id from t_service_name where service_name = #{service_name} and ip = #{ip} and port = #{port};
    </select>
    <insert id="insertServiceName" parameterType="serviceName">
        insert into t_service_name(service_name,ip,port) values(#{service_name},#{ip},#{port});
    </insert>
</mapper>