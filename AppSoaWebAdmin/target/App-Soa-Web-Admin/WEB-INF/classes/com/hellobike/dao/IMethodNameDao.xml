<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hellobike.dao.IMethodNameDao">
    <select id="findIdByMethodName" resultType="Integer" parameterType="string">
        select id from t_method_name where method_name = #{value} ;
    </select>
    <insert id="insertMethodName" parameterType="string">
        insert into t_method_name(method_name) values(#{value});
    </insert>
</mapper>