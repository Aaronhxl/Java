<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hellobike.dao.IInterfaceNameDao">
    <select id="findIdByInterfaceName" resultType="Integer" parameterType="string">
        select id from t_interface_name where interface_name = #{value} ;
    </select>
    <insert id="insertInterfaceName" parameterType="string">
        insert into t_interface_name(interface_name) values(#{value});
    </insert>
</mapper>